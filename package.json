		"lucide": "^0.561.0",
		"lucide-react": "^0.552.0",
		"next": "15.5.9",
		"next-themes": "^0.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-hook-form": "^7.66.0",
		"resend": "^6.8.0",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.3.1",
		"zustand": "^5.0.9"
