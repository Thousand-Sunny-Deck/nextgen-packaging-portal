{
	"name": "NextGen Packaging client portal",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev:app": "npx prisma generate && next dev --turbopack",
		"dev:job": "npx --ignore-scripts=false inngest-cli@latest dev",
