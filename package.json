		"eslint": "9.38.0",
		"eslint-config-next": "15.5.6",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier": "5.5.4",
		"husky": "9.1.7",
		"lint-staged": "16.2.4",
		"prettier": "^3.6.2",
		"prisma": "^7.3.0",
		"tailwindcss": "4.1.14",
		"tw-animate-css": "^1.4.0",
